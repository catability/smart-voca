);

-- 2. 학습 세션 테이블 (learning_sessions)
CREATE TABLE IF NOT EXISTS learning_sessions (
    session_id      INTEGER PRIMARY KEY AUTOINCREMENT,
    session_type    TEXT NOT NULL,             -- 세션 유형 (예: '암기', '시험')
