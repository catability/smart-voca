CREATE TABLE IF NOT EXISTS learning_history (
    history_id      INTEGER PRIMARY KEY AUTOINCREMENT,
    session_id      INTEGER NOT NULL,
    word_id         INTEGER NOT NULL,
    is_correct      INTEGER NOT NULL,          -- 정답 여부 (0:오답, 1:정답)
