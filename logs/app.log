CREATE TABLE IF NOT EXISTS exam_history (
    exam_id         INTEGER PRIMARY KEY AUTOINCREMENT,
    exam_date       TEXT NOT NULL,
    exam_type       TEXT NOT NULL,             -- 시험 유형 (예: '단답형', '객관식')
