-- 1. 단어 정보 테이블 (words)
CREATE TABLE IF NOT EXISTS words (
    word_id         INTEGER PRIMARY KEY AUTOINCREMENT,
    word_text       TEXT NOT NULL UNIQUE,      -- 단어 (예: 'apple')
    meaning_ko      TEXT NOT NULL,             -- 한국어 의미 (예: '사과')
